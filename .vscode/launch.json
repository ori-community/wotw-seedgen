    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "attach",
            "name": "Attach executable 'wotw_seedgen_cli'",
            "pid": "${command:pickMyProcess}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in 'wotw_seedgen'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--package=wotw_seedgen",
                ]
            },
            "cwd": "${workspaceFolder}/wotw_seedgen"
        }
    ]
}
