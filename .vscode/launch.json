{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "cppvsdbg",
      "request": "attach",
      "name": "Attach to CLI (C++)",
      "processId": "${command:pickProcess}"
    },
    {
      "type": "lldb",
      "request": "attach",
      "name": "Attach to CLI",
      "program": "${workspaceFolder}/seedgen.exe"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in 'wotw_seedgen'",
      "cargo": {
        "args": ["test", "--no-run", "--package=wotw_seedgen"]
      },
      "cwd": "${workspaceFolder}/wotw_seedgen"
    }
  ]
}
