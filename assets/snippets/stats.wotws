!state(previous_spirit_light_collected, Integer)

on change player.spiritLight {
    set_integer("delta", player.spiritLight - previous_spirit_light_collected)

    if get_integer("delta") > 0 store(spirit_light_collected, spirit_light_collected + get_integer("delta"))

    store(previous_spirit_light_collected, player.spiritLight)
}

!state(previous_keystones_collected, Integer)

on change player.keystones {
    set_integer("delta", player.keystones - previous_keystones_collected)

    if get_integer("delta") > 0 store(keystones_collected, keystones_collected + get_integer("delta"))

    store(previous_keystones_collected, player.keystones)
}

!state(previous_ore_collected, Integer)

on change player.gorlekOre {
    set_integer("delta", player.gorlekOre - previous_ore_collected)

    if get_integer("delta") > 0 store(ore_collected, ore_collected + get_integer("delta"))

    store(previous_ore_collected, player.gorlekOre)
}

on change marsh_pickups_collected store(marsh_pickups_completion, (marsh_pickups_collected - marsh_pickups_collected_on_spawn) * (1 / (total_marsh_pickups - marsh_pickups_collected_on_spawn)))
on change hollow_pickups_collected store(hollow_pickups_completion, (hollow_pickups_collected - hollow_pickups_collected_on_spawn) * (1 / (total_hollow_pickups - hollow_pickups_collected_on_spawn)))
