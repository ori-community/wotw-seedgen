!state(after_spawn, Boolean)
on change statsUberStateGroup.distanceTravelledStatSettingSerializedFloatUberState store(after_spawn, true)
on after_spawn {
    store(marsh_pickups_collected_on_spawn, marsh_pickups_collected)
    store(hollow_pickups_collected_on_spawn, hollow_pickups_collected)
    store(glades_pickups_collected_on_spawn, glades_pickups_collected)
    store(wellspring_pickups_collected_on_spawn, wellspring_pickups_collected)
    store(woods_pickups_collected_on_spawn, woods_pickups_collected)
    store(reach_pickups_collected_on_spawn, reach_pickups_collected)
    store(depths_pickups_collected_on_spawn, depths_pickups_collected)
    store(pools_pickups_collected_on_spawn, pools_pickups_collected)
    store(wastes_pickups_collected_on_spawn, wastes_pickups_collected)
    store(ruins_pickups_collected_on_spawn, ruins_pickups_collected)
    store(willow_pickups_collected_on_spawn, willow_pickups_collected)
    store(burrows_pickups_collected_on_spawn, burrows_pickups_collected)
    store(shop_pickups_collected_on_spawn, shop_pickups_collected)
    store(pickups_collected_on_spawn, pickups_collected)
    save()
}

