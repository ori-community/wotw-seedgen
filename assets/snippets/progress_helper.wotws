!builtin_icon(progress_summary_icon, "assets/icons/wheel/progress_summary.blue.png") // TODO inconsistent casing in the icon filenames
!builtin_icon(tree_progress_icon, "assets/icons/wheel/tree_progress.blue.png")
!builtin_icon(wisp_progress_icon, "assets/icons/wheel/wisps_progress.png") // TODO why do others have blue and this one not
!builtin_icon(quest_progress_icon, "assets/icons/wheel/quests_progress.blue.png") // TODO some filenames are singular, some plural...
!builtin_icon(teleporter_icon, "assets/icons/game/teleporter.png")

!export(write_progress_message)
fun write_progress_message() {
    set_integer("collected", pickups_collected)
    set_integer("collected_on_spawn", pickups_collected_on_spawn)
    set_integer("total", total_pickups)
    pickups_progress()
}

!export(write_map_progress_message)
fun write_map_progress_message() {
    set_string("progress_message", "")
    // TODO this breaks the zone abstraction
    if player.currentMapArea == 0 {
        set_integer("collected", marsh_pickups_collected)
        set_integer("collected_on_spawn", marsh_pickups_collected_on_spawn)
        set_integer("total", total_marsh_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 1 {
        set_integer("collected", hollow_pickups_collected)
        set_integer("collected_on_spawn", hollow_pickups_collected_on_spawn)
        set_integer("total", total_hollow_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 2 {
        set_integer("collected", glades_pickups_collected)
        set_integer("collected_on_spawn", glades_pickups_collected_on_spawn)
        set_integer("total", total_glades_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 3 {
        set_integer("collected", wellspring_pickups_collected)
        set_integer("collected_on_spawn", wellspring_pickups_collected_on_spawn)
        set_integer("total", total_wellspring_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 7 {
        set_integer("collected", woods_pickups_collected)
        set_integer("collected_on_spawn", woods_pickups_collected_on_spawn)
        set_integer("total", total_woods_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 6 {
        set_integer("collected", reach_pickups_collected)
        set_integer("collected_on_spawn", reach_pickups_collected_on_spawn)
        set_integer("total", total_reach_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 8 {
        set_integer("collected", depths_pickups_collected)
        set_integer("collected_on_spawn", depths_pickups_collected_on_spawn)
        set_integer("total", total_depths_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 4 {
        set_integer("collected", pools_pickups_collected)
        set_integer("collected_on_spawn", pools_pickups_collected_on_spawn)
        set_integer("total", total_pools_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 9 {
        set_integer("collected", wastes_pickups_collected)
        set_integer("collected_on_spawn", wastes_pickups_collected_on_spawn)
        set_integer("total", total_wastes_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 10 {
        set_integer("collected", ruins_pickups_collected)
        set_integer("collected_on_spawn", ruins_pickups_collected_on_spawn)
        set_integer("total", total_ruins_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 11 {
        set_integer("collected", willow_pickups_collected)
        set_integer("collected_on_spawn", willow_pickups_collected_on_spawn)
        set_integer("total", total_willow_pickups)
        pickups_progress()
    }
    if player.currentMapArea == 5 {
        set_integer("collected", burrows_pickups_collected)
        set_integer("collected_on_spawn", burrows_pickups_collected_on_spawn)
        set_integer("total", total_burrows_pickups)
        pickups_progress()
    }
}

fun pickups_progress() {
    set_string("progress_message", "Pickups: " + (get_integer("collected") - get_integer("collected_on_spawn")) + "/" + (get_integer("total") - get_integer("collected_on_spawn")))
    if get_integer("collected") >= get_integer("total") set_string("progress_message", "$" + get_string("progress_message") + "$")
}

!state(disable_keybinds, Boolean)
!export(disable_keybinds)

on reload {
    set_wheel_item_data(
        "root",
        Top,
        "Progress Summary",
        "[Ability1]View Progress",
        progress_summary_icon,
        trigger_keybind("ShowProgressWithHints"),
    )

    set_wheel_item_data(
        "root",
        TopRight,
        "Tree Progress",
        "[Ability1]View Progress",
        tree_progress_icon,
        {
            write_tree_progress()
            priority_message(get_string("tree_progress"))
        },
    )

    set_wheel_item_data(
        "root",
        RightTop,
        "Wisp Progress",
        "[Ability1]View Progress",
        wisp_progress_icon,
        {
            write_wisp_progress()
            priority_message(get_string("wisp_progress"))
        },
    )

    set_wheel_item_data(
        "root",
        Right,
        "Quest Progress",
        "[Ability1]View Progress",
        quest_progress_icon,
        show_quest_progress(),
    )

    set_wheel_item_data(
        "root",
        RightBottom,
        "Teleporter Progress",
        "[Ability1]View Progress",
        teleporter_icon,
        show_teleporter_progress(),
    )
}

on progress_message {
    write_progress_message()
    // TODO priority message shouldn't enter the Alt+T log
    priority_message(get_string("progress_message"))
}

on reload {
    free_message("map_message", "")
    set_message_position("map_message", 0.5, 0.84)
    set_message_vertical_anchor("map_message", Bottom)
    set_message_background("map_message", false)
}

// TODO we call this "zone", not "area"
on change player.currentMapArea {
    if player.currentMapArea < 12 {
        write_map_progress_message()
        // TODO are we supposed to use set_map_message? Didn't seem to work...
        set_message_text("map_message", "<s_0.9><ls_0.8>" + get_string("progress_message") + "</></>")
    }
    if player.currentMapArea >= 12 set_message_text("map_message", "")
}

on binding1 if disable_keybinds == false {
    write_tree_progress()
    write_wisp_progress()
    priority_message(get_string("tree_progress") + "\n" + get_string("wisp_progress"))
}
on binding2 if disable_keybinds == false show_quest_progress()
on binding3 if disable_keybinds == false show_teleporter_progress()

fun write_tree_progress() {
    set_string("tree_progress", "#Trees found#: ")

    set_boolean("found", true)
    tree_string()

    set_string("tree_progress", get_string("tree_progress") + "\n#Trees remaining#: ")

    set_boolean("found", false)
    tree_string()
}

fun tree_string() {
    set_boolean("comma", false)

    if MarshSpawn.RegenTree == get_boolean("found") {
        set_string("tree_progress", get_string("tree_progress") + "Regenerate")
        set_boolean("comma", true)
    }
    if HowlsDen.SwordTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Sword")
        set_boolean("comma", true)
    }
    if HowlsDen.DoubleJumpTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Double Jump")
        set_boolean("comma", true)
    }
    if MarshPastOpher.BowTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Bow")
        set_boolean("comma", true)
    }
    if WestHollow.DashTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Dash")
        set_boolean("comma", true)
    }
    if EastHollow.BashTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Bash")
        set_boolean("comma", true)
    }
    if InnerWellspring.GrappleTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Grapple")
        set_boolean("comma", true)
    }
    if MarshSpawn.DamageTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Marsh Ancestral Light")
        set_boolean("comma", true)
    }
    if GladesTown.DamageTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Glades Ancestral Light")
        set_boolean("comma", true)
    }
    if LowerWastes.BurrowTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Burrow")
        set_boolean("comma", true)
    }
    if UpperReach.LightBurstTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Grenade")
        set_boolean("comma", true)
    }
    if LowerDepths.FlashTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Flash")
        set_boolean("comma", true)
    }
    if UpperPools.SwimDashTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Water Dash")
        set_boolean("comma", true)
    }
    if WeepingRidge.LaunchTree == get_boolean("found") {
        trees_add_comma()
        set_string("tree_progress", get_string("tree_progress") + "Launch")
        set_boolean("comma", true)
    }

    if get_boolean("comma") == false {
        set_string("tree_progress", get_string("tree_progress") + "None")
    }
}

fun trees_add_comma() {
    if get_boolean("comma") set_string("tree_progress", get_string("tree_progress") + ", ")
}

