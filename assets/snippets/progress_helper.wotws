#category("Quality of Life")
#name("Progress Helper")
#description("
Tracks your progress as you play
By default those are bound to
- Alt+P (general progress)
- Ctrl+Alt+1 (trees)
- Ctrl+Alt+2 (wisps)
- Ctrl+Alt+3 (quests)
")

// TODO there's a regression here because previously progress messages were in a consistent order
// To add a progress message from your header, use the write_progress_message event and append your progress message to the string with id progress_message
// If you also want to add to the progress displayed over the map, use the write_map_progress_message event and append to the string with id progress_message
// Usually, you'll want to start with a newline so you have your own line
// This example shows how to add a line "Fun: 10/10" into both the progress message and the map:
// !include("progress_helper", write_progress_message, write_map_progress_message)
// !augment_fun(write_progress_message, set_string("progress_message", get_string("progress_message") + "\nFun: 10/10"))
// !augment_fun(write_map_progress_message, set_string("map_progress_message", get_string("map_progress_message") + "\nFun: 10/10"))

!include(
    "stats",
    marsh_pickups_collected, marsh_pickups_collected_on_spawn, total_marsh_pickups,
    hollow_pickups_collected, hollow_pickups_collected_on_spawn, total_hollow_pickups,
    glades_pickups_collected, glades_pickups_collected_on_spawn, total_glades_pickups,
    wellspring_pickups_collected, wellspring_pickups_collected_on_spawn, total_wellspring_pickups,
    woods_pickups_collected, woods_pickups_collected_on_spawn, total_woods_pickups,
    reach_pickups_collected, reach_pickups_collected_on_spawn, total_reach_pickups,
    depths_pickups_collected, depths_pickups_collected_on_spawn, total_depths_pickups,
    pools_pickups_collected, pools_pickups_collected_on_spawn, total_pools_pickups,
    wastes_pickups_collected, wastes_pickups_collected_on_spawn, total_wastes_pickups,
    ruins_pickups_collected, ruins_pickups_collected_on_spawn, total_ruins_pickups,
    willow_pickups_collected, willow_pickups_collected_on_spawn, total_willow_pickups,
    burrows_pickups_collected, burrows_pickups_collected_on_spawn, total_burrows_pickups,
    pickups_collected, pickups_collected_on_spawn, total_pickups
